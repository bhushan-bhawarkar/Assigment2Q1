      "source": [
        "def moveZeroes(nums):\n",
        "\n",
        "\n",
        "    last_non_zero_pos = 0\n",
        "\n",
        "\n",
        "    for i in range(len(nums)):\n",
        "        if nums[i] != 0:\n",
        "\n",
        "            nums[i], nums[last_non_zero_pos] = nums[last_non_zero_pos], nums[i]\n",
        "\n",
        "            last_non_zero_pos += 1\n",
        "\n",
        "\n",
        "nums = [0, 1, 0, 3, 12]\n",
        "moveZeroes(nums)\n",
        "print(nums)\n"
      ]
